{
	"name": "trailme",
	"version": "1.0.0",
	"description": "### Table of Contents",
	"main": "server.js",
	"dependencies": {
		"concurrently": "^5.3.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"if-env": "^1.0.4",
		"mongoose": "^5.10.7",
		"nodemon": "^2.0.4"
	},
	"devDependencies": {},
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "concurrently \"nodemon --ignore 'trail-me/*'\" \"npm run trail-me\"",
		"trail-me": "cd trail-me && npm run start",
		"install": "cd trail-me && npm install",
		"build": "cd trail-me && npm run build",
		"heroku-postbuild": "npm run build"
	},
